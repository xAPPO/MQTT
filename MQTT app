                }
                catch (e) {
			    }            
                count++
		   // }
	    }
     }
    MQTTRGBT = (settings?.colourT)  //separate as extra values
	if (MQTTRGBT != null){
                
        count=0
        MQTTRGBT.each {
            if (settings?.HADiscovery && (!publishList.containsKey(myName))){
                publishList.put(it.id,[myName,"light","colour"])
            }
       
